time:Instant
  a sh:NodeShape ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue code:CodeSystem_ISO8601 ;
      owl:onProperty code:hasCodeSystem ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path time:inXSDDate ;
