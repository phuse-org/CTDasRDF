study:agecalc
  a spin:Function ;
  spin:body [
      a sp:Select ;
      sp:resultVariables (
          [
            sp:varName "age" ;
          ]
        ) ;
      sp:where (
          [
            a sp:Bind ;
            sp:expression [
                a xsd:date ;
                sp:arg1 [
                    a sp:substr ;
                    sp:arg1 [
                        sp:varName "birthdate" ;
                      ] ;
                    sp:arg2 1 ;
                    sp:arg3 10 ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "bdate" ;
              ] ;
          ]
          [
            a sp:Bind ;
            sp:expression [
                a xsd:date ;
                sp:arg1 [
                    a sp:substr ;
                    sp:arg1 [
                        sp:varName "refdate" ;
                      ] ;
                    sp:arg2 1 ;
                    sp:arg3 10 ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "rfd" ;
              ] ;
          ]
          [
            a sp:Bind ;
            sp:expression [
                a smf:duration ;
                sp:arg1 "y" ;
                sp:arg2 [
                    sp:varName "bdate" ;
                  ] ;
                sp:arg3 [
                    sp:varName "rfd" ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "age" ;
              ] ;
          ]
        ) ;
    ] ;
  spin:constraint [
      a spl:Argument ;
      spl:predicate arg:birthdate ;
      spl:valueType xsd:string ;
      rdfs:comment "calculates age from birthdate and reference start date" ;
    ] ;
  spin:constraint [
      a spl:Argument ;
      spl:predicate arg:refdate ;
      spl:valueType xsd:string ;
    ] ;
  spin:returnType xsd:integer ;
  rdfs:comment "" ;
  rdfs:subClassOf spl:DateFunctions ;
  skos:definition "" ;
  skos:prefLabel "agecalc" ;
.
